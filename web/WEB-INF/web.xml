<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>tabla</servlet-name>
        <servlet-class>servlet.tabla</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>eliminarregistro</servlet-name>
        <servlet-class>servlet.eliminarregistro</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>editartabla</servlet-name>
        <servlet-class>servlet.editartabla</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>iniciodesesion</servlet-name>
        <servlet-class>servlet.iniciodesesion</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>iniciosesion</servlet-name>
        <servlet-class>servlet.iniciosesion</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>menuusuario</servlet-name>
        <servlet-class>servlet.menuusuario</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>tabla</servlet-name>
        <url-pattern>/tabla</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>eliminarregistro</servlet-name>
        <url-pattern>/eliminar</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>editartabla</servlet-name>
        <url-pattern>/editar</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>iniciodesesion</servlet-name>
        <url-pattern>/iniciodesesion</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>iniciosesion</servlet-name>
        <url-pattern>/iniciar</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>menuusuario</servlet-name>
        <url-pattern>/menuusuario</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
